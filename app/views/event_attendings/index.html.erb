<p id="notice"><%= notice %></p>

<h1>Event Attendings</h1>

<table>
  <thead>
    <tr>
      <th>Attended event</th>
      <th>Attending user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_attendings.each do |event_attending| %>
      <tr>
        <td><%= event_attending.attended_event_id %></td>
        <td><%= event_attending.attending_user_id %></td>
        <td><%= link_to 'Show', event_attending %></td>
        <td><%= link_to 'Edit', edit_event_attending_path(event_attending) %></td>
        <td><%= link_to 'Destroy', event_attending, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Attending', new_event_attending_path %>
