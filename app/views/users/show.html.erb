<p id="notice"><%= notice %></p>

<p>
  <table>
  <tr>
    <th>Name:</th>
    <th>Events Created by this User:</th>
    <th> Attended Events by this User:</th>
   
  </tr>
  <tr>
    <td> <%= @user.name %> </td>
    <td>   <ol> <% @user.created_events.each do |event| %>
    <li>
        <%= event.name %> 
        </li>
        <% end %> </ol>
    </td>
     <td>   <ol> <% @user.attended_events.each do |event| %>
    <li>
        <%= event.name %> 
        </li>
        <% end %> </ol>
    </td>
   
  </tr>
  </table>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
